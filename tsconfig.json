{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2020",
    "experimentalDecorators": true,
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "Node",
    "baseUrl": "./",
    // "allowUmdGlobalAccess": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "skipLibCheck": true
  }
}
